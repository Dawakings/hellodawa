<table class="table">
    <tr>
        <th>Antal</th>
        <th>Vara</th>
        <th>Pris</th>
    </tr>
        {%for vara in varaArray%}
            {{vara[0].pris}}
    <tr>
        <td>{{vara[1]}};</td>
        <td>{{vara[0].vara}};</td>
        <td>{{vara[0].pris|number_format(2,',',' ')}} kr</td>
    </tr>
